<!--New Book部分-->
<h2>New book</h2>
<!--form with部分-->
<%= form_with model: @book do |f| %>
  <h4>Title</h4>
  <%= f.text_field :title %>

  <h4>Opinion</h4>
  <%= f.text_area :body %>
  
  <input type="submit" name="commit" value="Create Book" class="btn btn-success" data-disabele-with="Create Book">

<% end %>